  },
  "engines": {
    "node": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.3",
    "express": "~4.17.1",
    "typescript": "^4.3.5"
  }
}